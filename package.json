{
  "name": "codemetrics",
  "version": "0.1.0",
  "description": "A highly pluggable system to get stats and code metrics from your projects",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel-node src",
    "dev" : "npm start -- --config ./tests/dev.config.js",
    "test": "babel-tape-runner tests/**/*.specs.js | tap-spec",
    "prepublish": "babel src --out-dir dist",
    "lint" : "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxdow/codemetrics.git"
  },
  "keywords": [
    "metrics",
    "analyse",
    "stats"
  ],
  "author": "Maxime Warnier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxdow/codemetrics/issues"
  },
  "homepage": "https://github.com/maxdow/codemetrics#readme",
  "dependencies": {
    "clor": "^1.0.2",
    "commander": "^2.8.1",
    "inquirer": "^0.9.0",
    "log-symbols": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-tape-runner": "^1.2.0",
    "intercept-stdout": "^0.1.2",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  }
}
